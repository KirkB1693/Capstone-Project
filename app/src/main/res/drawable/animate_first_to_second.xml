<animated-vector xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    tools:ignore="NewApi">
    <aapt:attr name="android:drawable">
        <vector
            android:name="vector"
            android:width="282dp"
            android:height="230dp"
            android:viewportWidth="282"
            android:viewportHeight="230">
            <path
                android:name="first_to_second"
                android:pathData="M 188 153 L 142 107"
                android:strokeColor="#1a237e"
                android:strokeWidth="5"
                android:strokeLineCap="round"/>
        </vector>
    </aapt:attr>
    <target android:name="first_to_second">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:propertyName="trimPathStart"
                    android:startOffset="25"
                    android:duration="975"
                    android:valueFrom="0"
                    android:valueTo="1"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/accelerate_interpolator"/>
                <objectAnimator
                    android:propertyName="trimPathEnd"
                    android:duration="975"
                    android:valueFrom="0"
                    android:valueTo="1"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/accelerate_interpolator"/>
            </set>
        </aapt:attr>
    </target>

</animated-vector>
